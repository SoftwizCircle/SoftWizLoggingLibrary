<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup Label="Globals">
    <SccProjectName></SccProjectName>
    <SccProvider></SccProvider>
    <SccAuxPath></SccAuxPath>
    <SccLocalPath></SccLocalPath>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageIcon>log.png</PackageIcon>
    <PackageIconUrl />
    <Description>SWC Logging is a logging library which helps us not only in logging but also provides us an in built user interface to search and analyze the log. This is very easy to integrate.</Description>
    <PackageId>SWCLogging</PackageId>
    <Product>SWCLogging</Product>
    <Company>SoftWiz Circle</Company>
    <Authors>kmrmanish86</Authors>
    <Owners>Manish Kumar</Owners>
    <Copyright>Copyright Â©  2023</Copyright>
    <NeutralLanguage></NeutralLanguage>
    <AssemblyVersion>1.0.9.1</AssemblyVersion>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Version>1.0.4</Version>
    <PackageTags>C#, Logging, .Net Core, MVC, Log, Lucene, ElasticSearch, ELMAH</PackageTags>
    <PackageReleaseNotes>- Ability to share search criteria so that same view could be executed on another machine
      - Constructing url for every log so that it could be easily shareable
      - Current version information is available on mouse over of copyright information
      - Corrected project url
      - Ability to execute search on click of legends</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/SoftwizCircle/SoftWizLoggingLibrary</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Lucene.Net" Version="4.8.0-beta00016" />
    <PackageReference Include="Lucene.Net.Analysis.Common" Version="4.8.0-beta00016" />
    <PackageReference Include="Lucene.Net.QueryParser" Version="4.8.0-beta00016" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Configuration" Version="7.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Update="SWC.html">
      <Pack>True</Pack>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="log.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="SWCLoggingLibrary.nuspec">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="log.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
